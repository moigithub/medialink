  "scripts": {
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node server/server.js",
    "dev": "NODE_ENV=development babel-node server/server.js",
    "prod": "NODE_ENV=production webpack -p node server/server.js",
    "lint": "node_modules/.bin/esw webpack.config.* client server",
    "lint:watch": "npm run lint -- --watch",
    "test": "NODE_ENV=testing mocha --compilers js:babel-register  --reporter spec \"client/**/*.test.js\"",
    "test2": "NODE_ENV=testing mocha --require babel-register --reporter spec \"client/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
